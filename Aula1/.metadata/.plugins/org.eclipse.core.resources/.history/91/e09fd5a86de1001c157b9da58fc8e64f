package sequencial;

import java.util.Scanner;

public class INSS {

	public static void main(String[] args) {
		
		double salarioBruto;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Insira o salário bruto: ");
		salarioBruto = sc.nextDouble();
		
		salarioBruto = salarioBruto * (1 - 0.11);
		
		System.out.printf("O salário após o desconto de 11% do INSS é de R$%.2f", salarioBruto);
		
		sc.close();

	}

}
